
@{
    ViewBag.Title = "Sucesso";
    Layout = "~/Views/Shared/_Chamado.cshtml";
    var message = ViewBag.message as string;
}

<div class="content content-center">
    <div class="container-fluid">
        <div class="row text-center">
            <div class="col-md-5">
                <div class="card">
                    <div class="card-body">
                        @if (@message != null)
                        {
                            <p>@message</p>
                        }
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>